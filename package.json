{
  "name": "discord-bot-api",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migrate:latest": "tsx src/database/migrate/bin",
    "gen:types": "kysely-codegen --camel-case --dialect sqlite --out-file src/database/types.ts && prettier --write src/database/types.ts",
    "migrate:gen": "npm run migrate:latest && npm run gen:types",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "migrate:make": "tsx src/utils/createMigration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/better-sqlite3": "^7.6.10",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.17.52",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "kysely-codegen": "^0.15.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
