    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^5.6.0",
    "axios": "^1.9.0",
    "better-sqlite3": "^9.5.0",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "kysely": "^0.27.3",
    "lodash": "^4.17.21",
    "sqlite": "^5.1.1",
